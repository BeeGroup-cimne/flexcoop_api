{
  "item_title": "comfortProfiles",
  "cache_control": "max-age=10,must-revalidate",
  "cache_expires": 10,
  "resource_methods": [
    "GET",
    "POST",
    "DELETE"
  ],
  "item_methods": [
    "GET",
    "PATCH",
    "PUT",
    "DELETE"
  ],
  "schema": {
    "venId": {
      "type": "string",
      "minlength": 1,
      "maxlength": 100,
      "required": true,
      "unique": true
    },
    "comfortType": {
      "type": "list",
      "allowed": [
        "Visual",
        "Thermal"
      ]
    },
    "visualComfortProfile": {
      "type": "dict",
      "schema": {
        "attr1": {
          "type": "float"
        },
        "attr2": {
          "type": "float"
        },
        "attr3": {
          "type": "float"
        }
      }
    },
    "thermalComfortProfile": {
      "type": "dict",
      "schema": {
        "attr1": {
          "type": "float"
        },
        "attr2": {
          "type": "float"
        },
        "attr3": {
          "type": "float"
        }
      }
    }
  }
}