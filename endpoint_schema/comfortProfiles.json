{
  "item_title": "comfortProfiles",
  "cache_control": "max-age=10,must-revalidate",
  "cache_expires": 10,
  "resource_methods": [
    "GET",
    "POST"
  ],
  "item_methods": [
  ],
  "schema": {
    "account_id": {
       "type": "uuid",
       "minlength": 36,
       "maxlength": 36,
       "required": true
     },
    "visualComfortProfile": {
      "type": "string",
      "allowed": [
                "Dim",
                "Less Brigth",
                "Neutral",
                "Bright",
                "Very Bright"
              ],
      "required": true
    },
    "thermalComfortHeat": {
      "type": "float",
      "required": true
    },
    "thermalComfortCool": {
      "type": "float",
      "required": true
    },
    "WaterProfile": {
      "type": "float",
      "min": 50,
      "max": 70,
      "required": true
    }
   }
  }