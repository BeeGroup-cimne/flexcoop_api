{
  "item_title": "derClusterForecastingRequest",
  "cache_control": "max-age=10,must-revalidate",
  "cache_expires": 10,
  "resource_methods": [
    "GET",
    "POST"
  ],
  "schema": {
    "type": "list",
      "required": "true",
      "schema": {
        "type": "dict",
        "schema": {
          "venId": {
            "type": "string",
            "minlength": 1,
            "maxlength": 100,
            "required": true,
            "unique": true
          }
        }
      },

    "horizon": {
      "type": "integer",
      "required": true
    },
    "horizonType": {
      "type": "list",
      "allowed": [
        "minutes",
        "hours"
      ]
    },
    "isp": {
      "type": "integer",
      "required": "true"
    },
    "ispType": {
      "type": "list",
      "allowed": [
        "minutes",
        "hours"
      ],
      "required": true
    }
  }
}
