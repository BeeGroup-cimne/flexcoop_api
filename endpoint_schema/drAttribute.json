{
  "item_title": "DRAttribute",
  "cache_control": "max-age=10,must-revalidate",
  "cache_expires": 10,
  "resource_methods": [
    "GET",
    "POST"
  ],
  "schema": {
    "osbId": {
      "type": "string",
      "minlength": 1,
      "maxlength": 100,
      "required": true,
      "unique": true
    },
    "derId": {
      "type": "string",
      "minlength": 1,
      "maxlength": 100,
      "required": true,
      "unique": true
    },
    "type": {
      "type": "list",
      "allowed": [
        "Light",
        "HVAC",
        "DHW"
      ]
    },
    "rampingUp": {
      "type": "number"
    },
    "deliveryPeriod": {
      "type": "number"
    },
    "deactivationPeriod": {
      "type": "number"
    }
  }
}