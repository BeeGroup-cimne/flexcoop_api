{
    "item_title": "vpp",
    "cache_control": "max-age=10,must-revalidate",
    "cache_expires": 10,
    "resource_methods": [
      "GET",
      "POST"
    ],
    "schema": {
        "cluster_id": {
		    "required": true,
            "type": "uuid",
            "minlength": 36,
            "maxlength": 36
        },
        "name": {
            "type": "string",
            "required": true
        },
        "reliability": {
            "type": "number",
            "required": true
        },
        "devices_id": {
            "type": "list",
            "schema": {
                "type": "uuid",
                "minlength": 36,
                "maxlength": 36,
                "required": true
            }
        },
        "ldems_id": {
            "type": "list",
            "schema": {
                "type": "uuid",
                "minlength": 36,
                "maxlength": 36,
                "required": true
            }
        },
        "upwards_flex": {
            "type": "number",
            "required": true
        },
        "downwards_flex": {
            "type": "number",
            "required": true
        },
        "aggregator_id": {
            "type": "string",
            "minlength": 3,
            "maxlength": 256,
            "required": true
        },
        "location": {
            "type": "string",
            "allowed": ["Spain","Netherlands"]
        },
        "criteria": {
            "type": "list",
            "schema": {
                "type": "string",
            "allowed": ["aFRR", "self-consumption", "wholesale"],
            "required": true
            }
        }       
    }
  }