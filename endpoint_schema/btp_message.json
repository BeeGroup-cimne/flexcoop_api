{
	"item_title": "btp_message",
	"id_field": "btp_messageId",
	"item_lookup_field": "btp_messageId",
	"item_url": "regex(\"[a-zA-Z0-9-]+\")",
	"cache_control": "max-age=10,must-revalidate",
	"cache_expires": 10,
	"resource_methods": [
		"POST",
		"GET"
	],
	"item_methods": [
		"GET",
		"PATCH",
		"DELETE"
	],
	"schema": {
		"btp_messageId": {
			"type": "uuid",
			"minlength": 36,
			"maxlength": 36,
			"required": true,
			"unique": true
		},
		"aggregator_id": {
			"type": "string",
			"required": true
		},
		"timestamp": {
			"type": "datetime",
			"required": true
		},
		"last_update": {
			"type": "datetime"
		},
		"status": {
			"type": "string",
			"allowed": [
				"placed",
				"modified",
				"activated",
				"finished"
			],
			"required": true
		},
		"btp_message": {
			"type": "dict",
			"schema": {
				"bsp": {
					"type": "uuid",
					"minlength": 36,
					"maxlength": 36,
					"required": true
				},
				"brp": {
					"type": "uuid",
					"minlength": 36,
					"maxlength": 36
				},
				"request": {
					"type": "integer"
				},
				"date_of_delivery": {
					"type": "datetime",
					"required": true
				}
			}
		}
	}
}