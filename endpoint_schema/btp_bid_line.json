{
	"item_title": "btp_bid_line",
	"item_lookup_field": "btp_messageId",
	"item_url": "regex(\"[a-zA-Z0-9-]+\")",
	"cache_control": "max-age=10,must-revalidate",
	"cache_expires": 10,
	"resource_methods": [
		"POST",
		"GET"
	],
	"item_methods":[
		"GET",
		"PATCH"
	],
	"schema": {
		"btp_messageId": {
			"type": "uuid",
			"minlength": 36,
			"maxlength": 36,
			"required": true
		},
		"reference": {
			"type": "uuid",
			"minlength": 36,
			"maxlength": 36,
			"required": true
		},
		"aggregator_id": {
			"type": "string",
			"required": true
		},
		"availability": {
			"type": "integer",
			"min": 1,
			"max": 100,
			"required": true
		},
		"start": {
			"type": "datetime",
			"required": true
		},
		"bid_price": {
			"type": "float",
			"required": true
		},
		"bid_flexibility": {
			"type": "float",
			"required": true
		},
		"last_update": {
			"type": "datetime"
		}
	}
}